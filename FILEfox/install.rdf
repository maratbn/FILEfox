<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>FILEfox@maratbn.com</em:id>
    <em:version>0.1</em:version>
    <em:type>2</em:type>
   
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0.8</em:minVersion>
        <em:maxVersion>3.6.*</em:maxVersion>
      </Description>
    </em:targetApplication>
   
    <!-- Front End MetaData -->
    <em:name>FILEfox</em:name>
    <em:description>
        Local file access proxy for JavaScript-based web applications running on Firefox.

        ** RECOMMENDED FOR EXPERIENCED USERS ONLY **

        For more information, please see 'Tools' -> 'FILEfox' -> 'About'
    </em:description>
    <em:creator>Marat Nepomnyashy</em:creator>
    <em:homepageURL>http://www.maratbn.com/FILEfox/</em:homepageURL>
  </Description>      
</RDF>
